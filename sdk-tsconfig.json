{
  "compilerOptions": {
    "module": "esnext",
    "noEmitHelpers": false,
    "importHelpers": true,
    "noResolve": false,
    "noEmit": true,
    "inlineSourceMap": false,
    "moduleResolution": "node",
    "target": "es5",
    "removeComments": true,
    "allowJs": true,
    "jsx": "react",
    "jsxFactory": "createComponent",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "lib": ["es5", "es2015.promise"],
    "types": [],
    "esModuleInterop": true
  }
}
